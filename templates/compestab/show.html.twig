{% extends 'lot.html.twig' %}
{% block body %}
    {% if is_granted("ROLE_ADMIN") or is_granted("ROLE_LOTPROV") %}
    <h1>Compestab</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ compestab.id }}</td>
            </tr>
            <tr>
                <th>Folio</th>
                <td>{{ compestab.folio }}</td>
            </tr>
            <tr>
                <th>Femitido</th>
                <td>{{ compestab.femitido ? compestab.femitido|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fimpreso</th>
                <td>{{ compestab.fimpreso ? compestab.fimpreso|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fentrega</th>
                <td>{{ compestab.fentrega ? compestab.fentrega|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fcancel</th>
                <td>{{ compestab.fcancel ? compestab.fcancel|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Sincostp</th>
                <td>{{ compestab.sincostp ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Duplicado</th>
                <td>{{ compestab.duplicado ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Importe</th>
                <td>{{ compestab.importe }}</td>
            </tr>
            <tr>
                <th>CSuspencion</th>
                <td>{{ compestab.cSuspencion }}</td>
            </tr>
            <tr>
                <th>FinicioSusp</th>
                <td>{{ compestab.finicioSusp ? compestab.finicioSusp|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FfinSusp</th>
                <td>{{ compestab.ffinSusp ? compestab.ffinSusp|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fvencimiento</th>
                <td>{{ compestab.fvencimiento ? compestab.fvencimiento|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('compestab_index') }}">back to list</a>

    <a href="{{ path('compestab_edit', {'id': compestab.id}) }}">edit</a>

    {{ include('compestab/_delete_form.html.twig') }}
    {% endif %}
    {% include 'compestab/comprobanteHeader.html.twig' %}
    <div class="container">
    {% include 'compestab/Detallesdelcomprobante.html.twig' %}
