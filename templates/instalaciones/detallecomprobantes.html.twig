<h1>Comprobantes <a class="badge badge-success badge-pill" href="{{ path('compestab_newbyinst', {'inst': instalacione.id }) }}"><i class="fa fa-plus"></i></a></h1>
<table class="table table-striped table-dark table-sm">
    <thead>
    <tr>
        <th>Folio</th>
        <th>Estado</th>
        <th>Duplicado</th>
        <th>Importe</th>
    </tr>
    </thead>
    <tbody>
    {% for comp in comprobantes %}
        <tr {% if comp.id == instalacione.compActual.id %} class="bg-primary"{% endif %}>
            <td id="{{ comp.id }}"><a href="{{ path('compestab_show', {'id': comp.id }) }}"
                                      class="badge badge-pill badge-light"> {{ comp.folio }}</a></td>
            <td>{{ comp.estadoComp }}</td>
            <td>{{ comp.duplicado ? 'Si' : 'No' }}</td>
            <td>{{ comp.importe }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>