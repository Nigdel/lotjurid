{% use "form_div_layout.html.twig" %}
{{ form_start(form) }}
<div class="form-group">
    <fieldset disabled>
        {{ form_errors(form) }}
        <div class="form-row m-0">
            <div class="form-group">
                {{ form_row(form.identidad) }}
            </div>
        </div>
        <div class="form-row m-0">
            <div class="form-group col-md-3">
                {{ form_row(form.idtipo) }}
            </div>
            <div class="form-group col-md-3">
                {{ form_row(form.idservicio) }}
            </div>
            <div class="form-group col-md-3">
                {{ form_row(form.idrama) }}
            </div>
            <div class="form-group col-md-3">
                {{ form_row(form.idextension) }}
            </div>
        </div>
        <div class="form-row m-0">
            {{ form_widget(form.dictamen) }}
        </div>
        <div class="d-none">
            {{ form_widget(form) }}
        </div>
    </fieldset>
</div>
{{ form_end(form) }}
