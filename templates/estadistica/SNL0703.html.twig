{% extends 'base2.html.twig' %}
{% block body %}
<h1 class="alert alert-primary">SNL-07-03 Informativo sobre Lots Consolidado <span class="badge badge-warning">{{ lots|length }}</span></h1>
    <div class="container">
        <div class="card">
            <div class="card-header">Lots Vigentes por Rama </div>
            <div class="card-body">
                <h1>Licencias Vigentes por Rama</h1>
                <table class="table table-striped table-dark table-sm" id="lotsv">
                    <tr>
                        <th>Rama</th>
                        <th>Total</th>
                        <th>Publico</th>
                        <th>Limitado</th>
                        <th>Propio</th>
                    </tr>

                    {% for lot in lotvigcargaSum %}
                        <tr>
                            <th>{{ lot.Ramas }}</th>
                            <td>{{ lot.Total }}</td>
                            <td>{{ lot.Publica }}</td>
                            <td>{{ lot.Limitada }}</td>
                            <td>{{ lot.Propia }}</td>
                        </tr>
                    {% else %}
                        <tr><td colspan="5">No se obtuvieron resultados</td></tr>
                    {% endfor %}

                </table>
            </div>
        </div>
